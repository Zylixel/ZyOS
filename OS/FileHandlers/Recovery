webSource = "https://raw.githubusercontent.com/Zylixel/ZyOS/master/OS/"


function download(url) --Move to Connection
	print(webSource.. "" ..url)
  local content = http.get(webSource.. "" ..url).readAll()
  if not content then
    blueScreen("Cannot connect to webpage", url)
  end
  f = fs.open(url, "w")
  f.write(content)
  f.close()
end